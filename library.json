{
  "name": "TMC-API",
  "version": "3.10.3",
  "description": "Official TRINAMIC API library",
  "keywords": [
    "tmc",
    "trinamic"
  ],
  "build": {
    "includeDir": ".",
    "srcDir": "tmc",
    "srcFilter": [
      "-<*>",
      "+<helpers/*>",
      "+<ramp/*>",
      "+<ic/**>",
      "-<ic/TMC2130>",
      "-<ic/TMC2160>",
      "-<ic/TMC2208>",
      "-<ic/TMC2224>",
      "-<ic/TMC2225>",
      "-<ic/TMC2226>",
      "-<ic/TMC2240>",
      "-<ic/TMC5031>",
      "-<ic/TMC5041>",
      "-<ic/TMC5062>",
      "-<ic/TMC5072>",
      "-<ic/TMC5160>",
      "-<ic/TMC5130>",
      "-<ic/TMC7300>",
      "-<**/Examples>",
      "-<**/*.png>",
      "-<**/*.md>"
    ]
  }
}
