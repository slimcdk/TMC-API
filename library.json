{
  "name": "TMC-API",
  "version": "3.10.3",
  "description": "Official TRINAMIC API library",
  "keywords": [
    "tmc",
    "trinamic"
  ],
  "build": {
    "includeDir": ".",
    "srcDir": "tmc",
    "srcFilter": [
      "-<*>",
      "+<helpers/*>",
      "+<ramp/*>",
      "+<ic/**>",
      "-<ic/TMC5062>",
      "-<ic/TMC5072>",
      "-<ic/TMC5160>",
      "-<ic/TMC5130>",
      "-<**/Examples>",
      "-<**/*.png>",
      "-<**/*.md>"
    ]
  }
}
